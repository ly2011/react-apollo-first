{
  "presets": [
    "es2015",
    "stage-2",
    "react"
  ],
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "lib",
        "style": true
      }
    ],
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-decorators"
  ],
  "comments": false
}